	(-0.5,0) .. (0,0.75) .. (0.5,0.5) .. (1,0.5) .. (2,0) .. (1,-0.5) .. (0.5,-0.5) .. (0,-0.75) .. (-0.5,0);
		\draw[use Hobby shortcut,closed=true,clockwise arrowsmulthole,fill=white,thick]
	(-2.75,-0.25) .. (-2,0.5) .. (-1.25,-0.25) .. (-2,-1) .. (-2.75,-0.25);
