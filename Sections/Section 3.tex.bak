	(4.5,4.5) .. (4,2) .. (3,-2) .. (0,-1) .. (-1.5,-1.5) .. (-1,0) .. (-2,3) .. (-1,4.5) .. (2,4) .. (4.5,4.5);
    \draw[use Hobby shortcut,closed=true,wise arrows,thick]
	(4.5,4.5) .. (4,2) .. (3,-2) .. (0,-1) .. (-1.5,-1.5) .. (-1,0) .. (-2,3) .. (-1,4.5) .. (2,4) .. (4.5,4.5);
